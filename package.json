{
  "name": "typescript-action",
  "version": "1.1.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build src/main.ts",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "duskmoon314",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/glob": "^0.3.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@typescript-eslint/parser": "^5.46.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.24.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.0.4",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.8.4"
  }
}